    <!DOCTYPE html>

    <html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>@ViewBag.Title</title>
        <link href="~/css/Site1css.css" rel="stylesheet" />
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
        <link href="~/lib/twitter-bootstrap/css/bootstrap.css" rel="stylesheet" />
        <script src="~/lib/jquery/jquery.js"></script>
        <script src="~/lib/jquery-validate/jquery.validate.js"></script>
        <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
        <script src="~/lib/twitter-bootstrap/js/bootstrap.js"></script>
        <script src="~/lib/bootbox/bootbox.min.js"></script>
        <style>
            .carousel-inner img {
                width: 1500px;
                height: 325px;
            }
        </style>
        @*<script src="https://cdn.ckeditor.com/4.14.1/standard/ckeditor.js"></script>*@
    </head>
    <body>
        <div class="" style="background-color:#E5D1AC">
            <div class="container table-borderless" style="background-color:azure; height: auto">
                <div class="navbar-nav">
                    <div>
                        <nav class="navbar navbar-expand-sm navbar-light " style="height: 25px; background-color:salmon">
                            <ul class="navbar-nav">
                                <li class="nav-item"><a class="nav-link" href="#">Page 1</a></li>
                                <li class="nav-item"><a class="nav-link" href="#">Page 2</a></li>
                                <li class="nav-item"><a class="nav-link" href="#">Page 3</a></li>
                            </ul>
                            <ul class=" navbar-nav ml-auto">
                                <li class="nav-item"><a class="nav-link" href="#">Page 4</a></li>
                                <li class="nav-item"><a class="nav-link" href="#">Page 5</a></li>
                            </ul>
                        </nav>
                        <nav class="navbar navbar-expand-sm  navbar-light" style="height:100px; background-color: #FED770">
                            <ul class="navbar-nav">
                                <li class="nav-item">
                                    <a class="nav-link" asp-controller="Home" asp-action="Index">
                                        <img class="rounded-circle" style="width:90px; height:90px" src="@Url.Content("~/images/Logo2.png")" alt="Alternate Text" />
                                    </a>
                                </li>
                            </ul>
                            <ul class="navbar-nav offset-2">
                                <li class="nav-item" style="width:500px; height:auto">
                                    <div class="form rounded-circle">
                                        <div class="row form-group">
                                            <div class="input-group md-form form-sm form-2 pl-0">
                                                <input class="form-control amber-border" type="search" placeholder="Search" aria-label="Search">
                                                <div class="input-group-append">
                                                    <button type="submit" class="btn-outline-primary text-xl-center"><i class="fa fa-search" aria-hidden="true"></i></button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                            <ul class="navbar-nav ml-auto">
                                <li class="nav-item">
                                    <a href="" class="text-black-50" style="width:auto; height: auto"><h5>Login</h5></a>
                                </li>
                                <li class="nav-item mx-2">
                                    <h5>|</h5>
                                </li>
                                <li class="nav-item">
                                    <a href="" class="text-black-50" style="width:auto; height: auto"><h5>View Cart <i class="fa fa-shopping-cart" aria-hidden="true" style=""></i></h5> </a>
                                </li>
                            </ul>

                        </nav>
                        <nav class="navbar navbar-expand-sm navbar-dark" style="height: 40px; background-color:#076A67">
                            <ul class="navbar-nav">
                                <li class="nav-item"><a class="nav-link" asp-controller="Home" asp-action="Management">Management</a></li>
                                <li class="nav-item"><a class="nav-link" href="#">Page 2</a></li>
                                <li class="nav-item"><a class="nav-link" href="#">Page 3</a></li>
                            </ul>
                            <ul class=" navbar-nav ml-auto">
                                <li class="nav-item"><a class="nav-link" href="#">Page 4</a></li>
                                <li class="nav-item"><a class="nav-link" asp-controller="Account" asp-action="Register">Register</a></li>
                            </ul>
                        </nav>
                    </div>
                    <nav class="navbar navbar-expand-sm" style="height: 350px ">

                        <div id="demo" class="carousel slide" data-ride="carousel">

                            <!-- Indicators -->
                            <ul class="carousel-indicators">
                                <li data-target="#demo" data-slide-to="0" class="active"></li>
                                <li data-target="#demo" data-slide-to="1"></li>
                                <li data-target="#demo" data-slide-to="2"></li>
                            </ul>

                            <!-- The slideshow -->
                            <div class="carousel-inner">
                                <div class="carousel-item active">
                                    <img class="nav-item img-thumbnail mt-2" src="@Url.Content("~/images/Banner10.png")" alt="Los Angeles">
                                </div>
                                <div class="carousel-item">
                                    <img class="nav-item img-thumbnail mt-2" src="@Url.Content("~/images/Banner20.png")" alt="Chicago">
                                </div>
                                <div class="carousel-item">
                                    <img class="nav-item img-thumbnail mt-2" src="@Url.Content("~/images/Banner3.jpg")" alt="New York">
                                </div>
                            </div>

                            <!-- Left and right controls -->
                            <a class="carousel-control-prev" href="#demo" data-slide="prev">
                                <span class="carousel-control-prev-icon"></span>
                            </a>
                            <a class="carousel-control-next" href="#demo" data-slide="next">
                                <span class="carousel-control-next-icon"></span>
                            </a>

                        </div>

                    </nav>
                </div>

                <div class="">
                    @RenderBody()
                </div>
                <div class="text-center py-4 text-light" style="background-color:#076A67 ">
                    ©@System.DateTime.Now.ToString("yyyy-MM-dd") Copyright: C0120G
                </div>
            </div>
        </div>
        @RenderSection("Script", false);
    </body>
    </html>
